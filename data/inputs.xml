<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<modded_inputs>
	<inputs>
		<actions>
			<!-- The display name for your input, note without using Community Framework you will need to make a localization file(stringtable.csv) and use the localization table for this -->
			<input name="QSEnableAutoWalk" loc="Enable Auto Walk" />
			<input name="QSEnableAutoJog" loc="Enable Auto Jog" />
			<input name="QSEnableAutoRun" loc="Enable Auto Run" />
		</actions>
	</inputs>
	<preset>
		<!-- The input name is the string used to refrence if they keybind has been pressed -->
		<input name="QSEnableAutoWalk">
			<btn name="kLControl">
				<btn name="kAdd" />
			</btn>
		</input>
		<input name="QSEnableAutoJog">
			<btn name="kAdd" />
		</input>
		<input name="QSEnableAutoRun">
			<btn name="kLShift">
				<btn name="kAdd" />
    	</btn>
		</input>
	</preset>
</modded_inputs>